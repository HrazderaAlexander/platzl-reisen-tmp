{
  "kind": "collectionType",
  "collectionName": "trip_dates",
  "info": {
    "singularName": "trip-date",
    "pluralName": "trip-dates",
    "displayName": "Reisetermin",
    "description": "Verfügbare Termine für Reisen"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "date": {
      "type": "string",
      "required": true,
      "maxLength": 50
    },
    "price": {
      "type": "integer",
      "required": true,
      "min": 0
    },
    "available": {
      "type": "boolean",
      "default": true
    },
    "sort_order": {
      "type": "integer",
      "default": 0
    },
    "trip": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::trip.trip",
      "inversedBy": "trip_dates"
    }
  }
}